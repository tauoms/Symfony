{% extends 'base.html.twig' %}

{% block title %}Formula 1 Race Data 2024
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<h1>Formula 1 Race Data 2024</h1>
		{# {{ data.MRData.RaceTable.Races.0.raceName }} #}
		{% for data in data.MRData.RaceTable.Races %}
			<li>{{data.raceName|e}}</li>
		{% endfor %}
	</div>
{% endblock %}
